{
    "julia.usePlotPane": false,
    "julia.additionalArgs": ["--threads=auto"],
    "[markdown]": {
        "editor.minimap.enabled": false
    },
    "[julia]": {
        "editor.detectIndentation": false,
        "editor.insertSpaces": true,
        "editor.tabSize": 4,
        "files.insertFinalNewline": true,
        "files.trimFinalNewlines": true,
        "files.trimTrailingWhitespace": true,
        "editor.rulers": [92],
    },
    "window.zoomLevel": 1,
    "latex-workshop.latex.recipe.default": "latexmk (xelatex) (escape)",
    "latex-workshop.latex.recipes": [
        {
            "name": "latexmk (xelatex) (escape)",
            "tools": [
                "xelatexmk (escape)"
            ]
        },
    ],
    "latex-workshop.latex.tools": [
        {
            "name": "xelatexmk (escape)",
            "command": "latexmk",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-xelatex",
                "-shell-escape",
                "-outdir=%OUTDIR%",
                "%DOC%"
            ],
            "env": {}
        }
    ]
}